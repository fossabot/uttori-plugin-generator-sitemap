{
  "name": "uttori-plugin-generator-sitemap",
  "version": "1.0.0",
  "description": "A plugin to generate an XML sitemap for search engines.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-plugin-generator-sitemap",
  "homepage": "https://github.com/uttori/uttori-plugin-generator-sitemap",
  "keywords": [
    "uttori",
    "sitemap"
  ],
  "engines": {
    "node": ">= 10.15.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "ramda": "^0.26.1",
    "uttori-utilities": "^1.6.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "coveralls": "^3.0.6",
    "docsify-cli": "^4.3.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^12.0.1",
    "eslint-plugin-xss": "^0.1.9",
    "jsdoc-to-markdown": "^5.0.1",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0"
  },
  "files": [
    "src/*"
  ],
  "main": "src/index.js",
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js src/index.js > docs/README.md",
    "docs-serve": "docsify serve ./docs",
    "lint": "eslint .",
    "report": "nyc report --reporter=html",
    "test": "NODE_ENV=test nyc ava --serial",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test ava --serial",
    "validate": "npm ls"
  }
}
